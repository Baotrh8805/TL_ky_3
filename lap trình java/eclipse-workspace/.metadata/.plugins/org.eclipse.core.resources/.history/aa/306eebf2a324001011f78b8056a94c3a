package daluong;


public class Syn {
	public void display() {
		for(int i=1;i<=10;i+=2) {
			System.out.print(i);
			try {
				Thread.sleep(1000);
				
			}catch (Exception ee) {}
		}
	}
}
class Nosyn extends Thread{
	Syn a=new Syn();
	public void run() {
		a.display();
	}
	
	public static void main (String []s) {
		Nosyn m=new Nosyn();
		Nosyn  n=new Nosyn();
		m.start();
		n.start();

	}
	
}

